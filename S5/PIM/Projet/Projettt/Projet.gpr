project Projet is

   for Main use ("adresse_ip.adb", "cache_la.adb", "cache_ll.adb", "cli.adb", "file_io.adb", "main.adb", "routeur.adb", "routeur_exceptions.ads", "routeur_la.adb", "routeur_ll.adb", "routeur_simple.adb", "routeur_outils");

   package Builder is
      for Default_Switches ("ada") use ("-s");
   end Builder;

   package Compiler is
      for Default_Switches ("ada") use ("-gnatwa", "-gnata", "-g");
   end Compiler;

   package Binder is
      for Default_Switches ("ada") use ("-E");
   end Binder;

end Projet;

